    Sub insertacompraarray(ByVal fechapar As String, ByVal paquete As Integer, ByVal asociado As Integer, ByVal puntospar As Integer, ByVal costopar As Integer, Optional ByVal patrocinador As Integer = 0, Optional ByVal padre As Integer = 0, Optional ByVal lado As String = "")
        Try

        
            Dim regresaasociado As Integer = Session("idasociado")
            If puntospar = 240 Then puntospar = 0

            If Not existeasociado(asociado) Then
                insertaasociado(asociado, puntospar, patrocinador, padre, lado, fechapar)
            Else
                If Not asociadoconexcedente(periodo, asociado) Then desactivaasociado(asociado)

            End If
            Dim idpaq As New List(Of Integer)
            Dim cantidades As New List(Of Integer)
            Dim puntos As New List(Of Integer)
            Dim costo As New List(Of Decimal)

            Session("idasociado") = asociado
            idpaq.Add(paquete)
            puntos.Add(puntospar)
            costo.Add(costopar)
            cantidades.Add(1)
            Dim strConnection As String = ConfigurationManager.ConnectionStrings("conexionKisem").ConnectionString
            Dim sqlConn As MySqlConnection = New MySqlConnection(strConnection)
            Dim strTeamQuery As String = ""
            Dim cmdFetchTeam As MySqlCommand = New MySqlCommand(strTeamQuery, sqlConn)
            Dim id As Integer = asociado
            Dim activacion As Integer = 1
            Dim bandera As Integer = 0
            For cont = 0 To idpaq.Count - 1
                'checa si está activo
                Dim activo As Boolean = checaactivo()
                Dim compraactivadora As Integer = 0
                Dim excedente As Integer = 0
                If Not activo And idpaq(cont) > 0 Then

                    If Not asociadoinactivoafuerza(asociado) Then
                        activaasociado(puntos(puntos.Count - 1))
                    Else
                        desactivaasociado(asociado)
                    End If

                    compraactivadora = 1
                Else
                    excedente = 1
                End If
                sqlConn.Open()
                strTeamQuery = "INSERT INTO compras(asociado, paquete, cantidad, puntos, costo, fecha, activacion, excedente, statuspago, statusentrega, total) VALUES(" & id.ToString & ", " & idpaq(cont).ToString & ", " & cantidades(cont).ToString & ", " & puntos(cont).ToString & ", " & costo(cont).ToString & ", '" & fechapar & "', " & compraactivadora.ToString & ", " & excedente.ToString & ", 'PAGADO', 'ENTREGADO', " & costo(cont).ToString & ")"

                cmdFetchTeam = New MySqlCommand(strTeamQuery, sqlConn)
                cmdFetchTeam.ExecuteNonQuery()


                sqlConn.Close()
                sqlConn.Dispose()
                Dim compra As Integer = recuperacompra(id)
                'agrega referencia a compra
                Dim referencia As Integer = funciones.referencia(compra)

                sqlConn.Open()

                strTeamQuery = "UPDATE compras SET referencia=" & referencia.ToString & " WHERE id=" & compra.ToString



                cmdFetchTeam = New MySqlCommand(strTeamQuery, sqlConn)
                cmdFetchTeam.ExecuteNonQuery()


                sqlConn.Close()
                sqlConn.Dispose()



                sqlConn.Open()
                strTeamQuery = "INSERT INTO comprasdetalle(compra, paquete, cantidad, puntos, costo) VALUES(" & compra.ToString & ", " & idpaq(cont).ToString & ", " & cantidades(cont).ToString & ", " & puntos(cont).ToString & ", " & costo(cont).ToString & ")"

                cmdFetchTeam = New MySqlCommand(strTeamQuery, sqlConn)
                cmdFetchTeam.ExecuteNonQuery()


                sqlConn.Close()
                sqlConn.Dispose()
                If puntos(cont) > 0 Then
                    If compraactivadora = 0 And idpaq(cont) = 3 Then
                        subepuntos(compra, puntos(cont) - 200, id)
                    Else
                        subepuntos(compra, puntos(cont), id)
                    End If

                End If
                Dim bodega As Integer = funciones.buscabodega(id)
                funciones.descuentadebodegas(bodega, idpaq(cont), cantidades(cont))
            Next
