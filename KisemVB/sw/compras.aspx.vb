        While dtrTeam.Read
            respuesta = True


        End While

        sqlConn.Close()
        sqlConn.Dispose()

        Return respuesta
    End Function
    Function Desordenar_array(ByRef vArray As Object) As Array

        Dim i As Long
        Dim rndIndex As Long
        Dim Temp As Object
        Dim startIndex As Integer
        Dim endIndex As Integer
        Randomize()

        startIndex = LBound(vArray)
        endIndex = UBound(vArray)

        For i = startIndex To endIndex
            rndIndex = Int((endIndex - startIndex + 1) * Rnd() + startIndex)

            Temp = vArray(i)
            vArray(i) = vArray(rndIndex)
            vArray(rndIndex) = Temp
        Next i
        Return vArray

    End Function

